  {{#grouped_each 3 this}}
  <div class="row">
    {{#each this}}
    <div class="col lg2 m4 s6">
      <div class="card">
        <div class="card-image"> 
          {{#if fb}}
          <span> 
            <div class="chip right"> 
              <i class="close material-icons">close</i> 
            </div> 
          </span>
          {{else}}
          {{/if}} 
          {{#if img}}
          <img class="cardImages" src="http://image.tmdb.org/t/p/w342/{{img}}" alt="{{title}}"> 
          {{else}}
          {{/if}}
        </div>
        <div class="card-content">
          <li class="cardTitle">{{title}}</li>
          <li class="cardDate">{{releaseDate}}</li>
          <li class="cardActors">
            {{#actors}}
              {{this}}
            {{/actors}}
          </li>
          {{#if boolian}}
            <li>{{boolian}}</li>
          {{/if}}
          {{#if ratings}}
            <li>{{rating}}</li>
          {{/if}} 
          {{#if mdn}}
          <a class="waves-effect waves-light btn blue-grey lighten-2 addtowatch"><i class="material-icons left">add</i>Watchlist</a>
          {{else}}
          <a class="waves-effect waves-light btn blue-grey lighten-2"><i class="material-icons left">add</i>STARS</a>
          {{/if}}
        </div>
      </div>
    </div>
{{/each}}

  </div>
  {{/grouped_each}}